#open jetstream instance

#run singularity
singularity

#git clone repo with singularity file
git clone -b https://github.com/TeamMango/cavatica.git

#move into that folder
cd diamondsingularity

#see whats in singularity file
ls
cat Singularity

#build singularity file
sudo singularity build diamond.smg Singularity

#run singularity file
singularity run diamond.simg
